    "apollo-server": "^3.13.0",
    "apollo-server-express": "^3.13.0",
    "axios": "^1.6.7",
    "bull": "^4.16.5",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.21.1",
    "graphql": "^16.10.0",
    "graphql-tag": "^2.12.6",
    "ioredis": "^5.6.0",
    "jsonwebtoken": "^9.0.2",
    "next": "14.1.0",
    "pg": "^8.14.1",
    "react": "^18",
